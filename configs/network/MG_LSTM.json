{ "NetworkName" : "DefaultCtF",
  "NetworkBuilder":"network",
  "NetworkStructure":{
    "Shared":[
      { "layerType":"Conv2D",
        "layerName":"Conv2D_1",
        "layerInput":"input.state",
        "Parameters":{"filters":16,
                      "kernel_size":3,
                      "strides":2,
                      "activation":"relu"}},
      { "layerType":"Conv2D",
        "layerName":"Conv2D_2",
        "layerInput":"Conv2D_1",
        "Parameters":{"filters":32,
                      "kernel_size":3,
                      "strides":2,
                      "activation":"relu"}},
      { "layerType":"Flatten",
        "layerName":"Flatten",
        "layerInput":"Conv2D_2"},
      { "layerType":"Reshape",
        "layerName":"Reshape",
        "layerInput":"Flatten",
        "Parameters":{"target_shape":[1,32]}},
      { "layerType":"LSTM",
        "layerName":"LSTM1",
        "layerInput":["Reshape","input.hiddenState"],
        "multiOutput":["LSTM1","hidden_s","hidden_c"],
        "Parameters":{"units":256,
                      "activation":"relu",
                      "recurrent_activation":"sigmoid",
                      "return_state":true}},
      { "layerType":"Dense",
        "layerName":"Dense1",
        "layerInput":"LSTM1",
        "Parameters":{"units":128,
                      "activation":null}}
      ],

    "Actor":[
      { "layerType":"Dense",
        "layerName":"Actor_FC",
        "layerInput":"Dense1",
        "Parameters":{"units":"actionSize",
                      "activation":null}},
      { "layerType":"SoftMax",
        "layerName":"Logits",
        "layerInput":"Actor_FC"},
      { "layerType":"LogSoftMax",
        "layerName":"LogLogits",
        "layerInput":"Actor_FC"}
    ],

    "Critic":[
      { "layerType":"Dense",
        "layerName":"Critic_FC1",
        "layerInput":"Dense1",
        "Parameters":{"units":1,
                      "activation":null}}
    ]
  },
  "NetworkOutputs":{
    "actor":"Logits",
    "critic":"Critic_FC1",
    "log_logits":"LogLogits",
    "hiddenState":"hidden_s",
    "hiddenCell":"hidden_c"
  },
  "NetworkVariableGroups":{
    "Actor":["Actor","Shared"],
    "Critic":["Critic","Shared"]
  },
  "TestInput":{ "state":[null,7,7,4],
                "hiddenState":256}

  }
