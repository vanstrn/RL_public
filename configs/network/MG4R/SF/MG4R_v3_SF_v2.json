{ "NetworkName" : "MG_SF",
  "NetworkBuilder":"network",
  "DefaultParams":{ "SFSize":128,
                    "Trainable":true,
                    "InFilter":64,
                    "OutFilter":64,
                    "FCUnits":256},
  "Inputs":{"state":[29,29,2]},
  "NetworkStructure":{
    "Shared":[
      { "layerType":"Conv2D",
        "layerName":"Conv2D_1",
        "layerInput":"input.state",
        "Parameters":{"filters":"InFilter",
                      "kernel_size":6,
                      "strides":2,
                      "padding":"same",
                      "activation":"relu",
                      "trainable":"Trainable"}},
      { "layerType":"Conv2D",
        "layerName":"Conv2D_2",
        "layerInput":"Conv2D_1",
        "Parameters":{"filters":"InFilter",
                      "kernel_size":6,
                      "strides":2,
                      "padding":"same",
                      "activation":"relu",
                      "trainable":"Trainable"}},
      { "layerType":"Inception",
        "layerName":"Inception1",
        "layerInput":"Conv2D_2",
        "Parameters":{"filters":[16,16,8,8],
                      "trainable":"Trainable"}},
      { "layerType":"Inception",
        "layerName":"Inception2",
        "layerInput":"Inception1",
        "Parameters":{"filters":[32,32,16,16],
                      "trainable":"Trainable"}},
      { "layerType":"Inception",
        "layerName":"Inception2",
        "layerInput":"Inception1",
        "Parameters":{"filters":[64,64,32,32],
                      "trainable":"Trainable"}},
      { "layerType":"Inception",
        "layerName":"Inception3",
        "layerInput":"Inception2",
        "Parameters":{"filters":[128,128,64,64],
                      "trainable":"Trainable"}},
      { "layerType":"GlobalAveragePooling2D",
        "layerName":"AveragePool",
        "layerInput":"Inception3",
        "Parameters":{"filters":[5,5]}},
      { "layerType":"Flatten",
        "layerName":"Flatten",
        "layerInput":"AveragePool"},
      { "layerType":"Dense",
        "layerName":"Dense1",
        "layerInput":"Flatten",
        "Parameters":{"units":"FCUnits",
                      "activation":"relu",
                      "trainable":"Trainable"}},
      { "layerType":"Dense",
        "layerName":"Phi",
        "layerInput":"Dense1",
        "Parameters":{"units":"SFSize",
                      "activation":"relu",
                      "trainable":"Trainable"}}
      ],

    "SucessorRepresentation":[
      { "layerType":"Dense",
        "layerName":"SuccessorWeights",
        "layerInput":"Phi",
        "Parameters":{"units":1,
                      "activation":null,
                      "trainable":"Trainable"}}
      ],


    "Critic":[
      { "layerType":"Dense",
        "layerName":"Critic_FC1",
        "layerInput":"Phi",
        "Parameters":{"units":"FCUnits",
                      "activation":"relu"}},
      { "layerType":"Dense",
        "layerName":"Critic_FC2",
        "layerInput":"Critic_FC1",
        "Parameters":{"units":"FCUnits",
                      "activation":"relu"}},
      { "layerType":"Dense",
        "layerName":"Psi",
        "layerInput":"Critic_FC2",
        "Parameters":{"units":"SFSize",
                      "activation":null}}
    ],

    "Critic_SF":[
      { "layerType":"Dense",
        "layerName":"Critic",
        "layerInput":"Psi",
        "ReuseLayer":"SuccessorWeights"}
    ],

    "Reconstruction":[
      { "layerType":"Dense",
        "layerName":"Reconstruction_Dense",
        "layerInput":"Phi",
        "Parameters":{"units":400,
                      "activation":"relu"}},
      { "layerType":"Reshape",
        "layerName":"Reconstruction_Reshape",
        "Parameters":{"target_shape":[5,5,16]},
        "layerInput":"Reconstruction_Dense"},
      { "layerType":"Conv2DTranspose",
        "layerName":"Reconstruction_Conv1",
        "layerInput":"Reconstruction_Reshape",
        "Parameters":{"filters":"OutFilter",
                      "kernel_size":3,
                      "strides":1,
                      "activation":"elu"}},
      { "layerType":"Conv2DTranspose",
        "layerName":"Reconstruction_Conv2",
        "layerInput":"Reconstruction_Conv1",
        "Parameters":{"filters":"OutFilter",
                      "kernel_size":3,
                      "strides":1,
                      "activation":"elu"}},
      { "layerType":"Conv2DTranspose",
        "layerName":"Reconstruction_Conv3",
        "layerInput":"Reconstruction_Conv2",
        "Parameters":{"filters":"OutFilter",
                      "kernel_size":3,
                      "strides":1,
                      "activation":"elu"}},
      { "layerType":"Conv2DTranspose",
        "layerName":"StatePrediction",
        "layerInput":"Reconstruction_Conv3",
        "Parameters":{"filters":2,
                      "kernel_size":3,
                      "strides":1,
                      "activation":null}}
    ]

  },
  "NetworkOutputs":[
    {
      "prediction":"StatePrediction",
      "reward_pred":"SuccessorWeights"
    },
    {
      "psi":"Psi"
    },
    {
      "phi":"Phi"
    },
    {
      "critic":"Critic"
    },
    {
      "phi":"Phi",
      "prediction":"StatePrediction",
      "reward_pred":"SuccessorWeights",
      "psi":"Psi",
      "critic":"Critic"
    }
  ],
  "NetworkInputs":[
    ["state","action"],
    ["state"],
    ["state"],
    ["state"],
    ["state","action"]
  ],

  "NetworkVariableGroups":{
    "Actor":["Actor"],
    "Critic":["Critic"],
    "Reward":["SucessorRepresentation","Shared"],
    "Reconstruction":["Reconstruction","Shared"]
  },
  "TestInput":{"state":[null,7,7,4]}

  }
