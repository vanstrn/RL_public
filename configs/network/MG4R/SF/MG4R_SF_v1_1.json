{ "NetworkName" : "MG_SF",
  "NetworkBuilder":"network",
  "DefaultParams":{ "SFSize":64,
                    "Trainable":true,
                    "InFilter":64,
                    "OutFilter":16,
                    "FCUnits":256},
  "Inputs":{"state":[19,19,2]},
  "NetworkStructure":{
    "Shared":[
      { "layerType":"Conv2D",
        "layerName":"Conv2D_1",
        "layerInput":"input.state",
        "Parameters":{"filters":"InFilter//4",
                      "kernel_size":3,
                      "strides":1,
                      "activation":"elu",
                      "trainable":"Trainable"}},
      { "layerType":"Conv2D",
        "layerName":"Conv2D_2",
        "layerInput":"Conv2D_1",
        "Parameters":{"filters":"InFilter//2",
                      "kernel_size":3,
                      "strides":1,
                      "activation":"elu",
                      "trainable":"Trainable"}},
      { "layerType":"Conv2D",
        "layerName":"Conv2D_3",
        "layerInput":"Conv2D_2",
        "Parameters":{"filters":"InFilter",
                      "kernel_size":3,
                      "strides":1,
                      "activation":"elu",
                      "trainable":"Trainable"}},
      { "layerType":"Conv2D",
        "layerName":"Conv2D_4",
        "layerInput":"Conv2D_3",
        "Parameters":{"filters":"InFilter",
                      "kernel_size":3,
                      "strides":1,
                      "activation":"elu",
                      "trainable":"Trainable"}},
      { "layerType":"Flatten",
        "layerName":"Flatten",
        "layerInput":"Conv2D_4"},
      { "layerType":"Dense",
        "layerName":"Dense1",
        "layerInput":"Flatten",
        "Parameters":{"units":"FCUnits",
                      "activation":"relu",
                      "trainable":"Trainable"}},
      { "layerType":"Dense",
        "layerName":"Phi",
        "layerInput":"Dense1",
        "Parameters":{"units":"SFSize",
                      "activation":"relu",
                      "trainable":"Trainable"}}
      ],

    "SucessorRepresentation":[
      { "layerType":"Dense",
        "layerName":"SuccessorWeights",
        "layerInput":"Phi",
        "Parameters":{"units":1,
                      "activation":null,
                      "trainable":"Trainable"}}
      ],


    "Critic":[
      { "layerType":"StopGradient",
        "layerName":"SG",
        "layerInput":"Phi",
        "Parameters":{}},
      { "layerType":"Dense",
        "layerName":"Critic_FC1",
        "layerInput":"SG",
        "Parameters":{"units":"FCUnits",
                      "activation":"relu"}},
      { "layerType":"Dense",
        "layerName":"Critic_FC2",
        "layerInput":"Critic_FC1",
        "Parameters":{"units":"FCUnits",
                      "activation":"relu"}},
      { "layerType":"Dense",
        "layerName":"Psi",
        "layerInput":"Critic_FC2",
        "Parameters":{"units":"SFSize",
                      "activation":null}}
    ],

    "Critic_SF":[
      { "layerType":"Dense",
        "layerName":"Critic",
        "layerInput":"Psi",
        "ReuseLayer":"SuccessorWeights"}
    ],

    "Reconstruction":[
      { "layerType":"Dense",
        "layerName":"Reconstruction_Dense",
        "layerInput":"Phi",
        "Parameters":{"units":968,
                      "activation":"relu"}},
      { "layerType":"Reshape",
        "layerName":"Reconstruction_Reshape",
        "Parameters":{"target_shape":[11,11,8]},
        "layerInput":"Reconstruction_Dense"},
      { "layerType":"Conv2DTranspose",
        "layerName":"Reconstruction_Conv1",
        "layerInput":"Reconstruction_Reshape",
        "Parameters":{"filters":"OutFilter",
                      "kernel_size":3,
                      "strides":1,
                      "activation":"elu"}},
      { "layerType":"Conv2DTranspose",
        "layerName":"Reconstruction_Conv2",
        "layerInput":"Reconstruction_Conv1",
        "Parameters":{"filters":"OutFilter",
                      "kernel_size":3,
                      "strides":1,
                      "activation":"elu"}},
      { "layerType":"Conv2DTranspose",
        "layerName":"Reconstruction_Conv3",
        "layerInput":"Reconstruction_Conv2",
        "Parameters":{"filters":"OutFilter",
                      "kernel_size":3,
                      "strides":1,
                      "activation":"elu"}},
      { "layerType":"Conv2DTranspose",
        "layerName":"StatePrediction",
        "layerInput":"Reconstruction_Conv3",
        "Parameters":{"filters":2,
                      "kernel_size":3,
                      "strides":1,
                      "activation":null}}
    ]

  },
  "NetworkOutputs":
    {
      "phi":"Phi",
      "prediction":"StatePrediction",
      "reward_pred":"SuccessorWeights",
      "psi":"Psi",
      "critic":"Critic"
    },

  "NetworkVariableGroups":{
    "Actor":["Actor"],
    "Critic":["Critic"],
    "Reward":["SucessorRepresentation","Shared"],
    "Reconstruction":["Reconstruction","Shared"]
  },
  "TestInput":{"state":[null,7,7,4]}

  }
