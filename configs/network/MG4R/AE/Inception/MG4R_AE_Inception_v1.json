{ "NetworkName" : "MG_AE_Inception_v1",
  "NetworkBuilder":"network",
  "DefaultParams":{"SFSize":128},
  "NetworkStructure":{
    "Shared":[
      { "layerType":"Inception",
        "layerName":"Inception1",
        "layerInput":"input.state",
        "Parameters":{"filters":[8,8,4,4]}},
      { "layerType":"Inception",
        "layerName":"Inception2",
        "layerInput":"Inception1",
        "Parameters":{"filters":[16,16,8,8]}},
      { "layerType":"Inception",
        "layerName":"Inception3",
        "layerInput":"Inception2",
        "Parameters":{"filters":[32,32,16,16]}},
      { "layerType":"Inception",
        "layerName":"Inception4",
        "layerInput":"Inception3",
        "Parameters":{"filters":[64,64,32,32]}},
      { "layerType":"Flatten",
        "layerName":"Flatten",
        "layerInput":"Inception4",
        "Parameters":{}},
      { "layerType":"Dense",
        "layerName":"Dense1",
        "layerInput":"Flatten",
        "Parameters":{"units":1024,
                      "activation":"relu"}},
      { "layerType":"Dense",
        "layerName":"Dense2",
        "layerInput":"Dense1",
        "Parameters":{"units":"SFSize",
                      "activation":null}}
      ],

    "Reconstruction":[
      { "layerType":"Dense",
        "layerName":"Action_Dense",
        "layerInput":"input.action",
        "Parameters":{"units":128,
                      "activation":"relu"}},
      { "layerType":"Concatenate",
        "layerName":"Reconstruction_Conc",
        "layerInput":["Dense1","Action_Dense"],
        "Parameters":{}},
      { "layerType":"Dense",
        "layerName":"Reconstruction_Dense",
        "layerInput":"Reconstruction_Conc",
        "Parameters":{"units":1024,
                      "activation":"relu"}},
      { "layerType":"Reshape",
        "layerName":"Reconstruction_Reshape",
        "Parameters":{"target_shape":[8,8,16]},
        "layerInput":"Reconstruction_Dense"},
      { "layerType":"Conv2DTranspose",
        "layerName":"Reconstruction_Conv1",
        "layerInput":"Reconstruction_Reshape",
        "Parameters":{"filters":64,
                      "kernel_size":3,
                      "strides":2,
                      "activation":"relu"}},
      { "layerType":"Conv2DTranspose",
        "layerName":"Reconstruction_Conv2",
        "layerInput":"Reconstruction_Conv1",
        "Parameters":{"filters":64,
                      "kernel_size":2,
                      "strides":1,
                      "activation":"relu"}},
      { "layerType":"Conv2DTranspose",
        "layerName":"Reconstruction_Conv3",
        "layerInput":"Reconstruction_Conv2",
        "Parameters":{"filters":2,
                      "kernel_size":2,
                      "strides":1,
                      "activation":"relu"}},
      { "layerType":"Round",
        "layerName":"Reconstruction_Round",
        "layerInput":"Reconstruction_Conv3",
        "Parameters":{"interval":0.25}}
    ]

  },
  "NetworkOutputs":{
    "prediction":"Reconstruction_Round",
    "phi":"Dense2"
  },

  "NetworkVariableGroups":{
    "Reconstruction":["Reconstruction","Shared"]
  },
  "TestInput":{"state":[null,7,7,4]}

  }
