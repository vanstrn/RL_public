{ "NetworkName" : "MG_SF",
  "NetworkBuilder":"network",
  "DefaultParams":{"SFSize":128},
  "NetworkStructure":{
    "Shared":[
      { "layerType":"Conv2D",
        "layerName":"Conv2D_1",
        "layerInput":"input.state",
        "Parameters":{"filters":64,
                      "kernel_size":3,
                      "strides":2,
                      "activation":"relu"}},
      { "layerType":"Conv2D",
        "layerName":"Conv2D_2",
        "layerInput":"Conv2D_1",
        "Parameters":{"filters":64,
                      "kernel_size":2,
                      "strides":2,
                      "activation":"relu"}},
      { "layerType":"Conv2D",
        "layerName":"Conv2D_3",
        "layerInput":"Conv2D_2",
        "Parameters":{"filters":64,
                      "kernel_size":2,
                      "strides":1,
                      "activation":"relu"}},
      { "layerType":"Flatten",
        "layerName":"Flatten",
        "layerInput":"Conv2D_3",
        "Parameters":{}},
      { "layerType":"Dense",
        "layerName":"Dense1",
        "layerInput":"Flatten",
        "Parameters":{"units":1024,
                      "activation":"relu"}},
      { "layerType":"Dense",
        "layerName":"Dense2",
        "layerInput":"Dense1",
        "Parameters":{"units":"SFSize",
                      "activation":null}}
      ],

    "Reconstruction":[
      { "layerType":"Dense",
        "layerName":"Reconstruction_Dense",
        "layerInput":"Dense1",
        "Parameters":{"units":1024,
                      "activation":"relu"}},
      { "layerType":"Reshape",
        "layerName":"Reconstruction_Reshape",
        "Parameters":{"target_shape":[8,8,16]},
        "layerInput":"Reconstruction_Dense"},
      { "layerType":"Conv2DTranspose",
        "layerName":"Reconstruction_Conv1",
        "layerInput":"Reconstruction_Reshape",
        "Parameters":{"filters":64,
                      "kernel_size":3,
                      "strides":2,
                      "activation":"relu"}},
      { "layerType":"Conv2DTranspose",
        "layerName":"Reconstruction_Conv2",
        "layerInput":"Reconstruction_Conv1",
        "Parameters":{"filters":64,
                      "kernel_size":2,
                      "strides":1,
                      "activation":"relu"}},
      { "layerType":"Conv2DTranspose",
        "layerName":"Reconstruction_Conv3",
        "layerInput":"Reconstruction_Conv2",
        "Parameters":{"filters":2,
                      "kernel_size":2,
                      "strides":1,
                      "activation":"relu"}},
      { "layerType":"Round",
        "layerName":"Reconstruction_Round",
        "layerInput":"Reconstruction_Conv3",
        "Parameters":{"interval":0.25}}
    ]

  },
  "NetworkOutputs":{
    "prediction":"Reconstruction_Round",
    "phi":"Dense2"
  },

  "NetworkVariableGroups":{
    "Reconstruction":["Reconstruction","Shared"]
  },
  "TestInput":{"state":[null,7,7,4]}

  }
