{ "NetworkName" : "MG4R_PPO",
  "NetworkBuilder":"network",
  "NetworkStructure":{
    "Shared":[
      { "layerType":"BasicCNNSplit",
        "layerName":"InputSplit",
        "layerInput":"input.state",
        "multiOutput":["in_background","in_agent"],
        "Parameters":{"division":1}},
      { "layerType":"Conv2D",
        "layerName":"Conv2D_1",
        "layerInput":"in_background",
        "Parameters":{"filters":32,
                      "kernel_size":6,
                      "strides":2,
                      "activation":"relu"}},
      { "layerType":"Conv2D",
        "layerName":"Conv2D_2",
        "layerInput":"Conv2D_1",
        "Parameters":{"filters":64,
                      "kernel_size":4,
                      "strides":1,
                      "activation":"relu"}},
      { "layerType":"Conv2D",
        "layerName":"Conv2D_3",
        "layerInput":"Conv2D_2",
        "Parameters":{"filters":128,
                      "kernel_size":3,
                      "strides":1,
                      "activation":"relu"}},
      { "layerType":"Flatten",
        "layerName":"Flatten",
        "layerInput":"Conv2D_3"},

      { "layerType":"Conv2D",
        "layerName":"Conv2D_1_agent",
        "layerInput":"in_agent",
        "Parameters":{"filters":16,
                      "kernel_size":6,
                      "strides":2,
                      "activation":"relu"}},
      { "layerType":"Conv2D",
        "layerName":"Conv2D_2_agent",
        "layerInput":"Conv2D_1_agent",
        "Parameters":{"filters":32,
                      "kernel_size":4,
                      "strides":1,
                      "activation":"relu"}},
      { "layerType":"Conv2D",
        "layerName":"Conv2D_3_agent",
        "layerInput":"Conv2D_2_agent",
        "Parameters":{"filters":64,
                      "kernel_size":3,
                      "strides":1,
                      "activation":"relu"}},
      { "layerType":"Flatten",
        "layerName":"Flatten_agent",
        "layerInput":"Conv2D_3_agent"},

      { "layerType":"Concatenate",
        "layerName":"Concatenate",
        "layerInput":["Flatten","Flatten_agent"],
        "Parameters":{}},

      { "layerType":"Dense",
        "layerName":"Dense1",
        "layerInput":"Concatenate",
        "Parameters":{"units":512,
                      "activation":"relu"}},
      { "layerType":"Dense",
        "layerName":"Dense2",
        "layerInput":"Dense1",
        "Parameters":{"units":512,
                      "activation":null}}
      ],
    "Actor":[
      { "layerType":"Dense",
        "layerName":"Actor_FC_1",
        "layerInput":"Dense2",
        "Parameters":{"units":512,
                      "activation":"relu"}},
      { "layerType":"Dense",
        "layerName":"Actor_FC_2",
        "layerInput":"Actor_FC_1",
        "Parameters":{"units":512,
                      "activation":"relu"}},
      { "layerType":"Dense",
        "layerName":"Actor_FC",
        "layerInput":"Actor_FC_2",
        "Parameters":{"units":"actionSize",
                      "activation":null}},
      { "layerType":"StopNan",
        "layerName":"StopNan",
        "layerInput":"Actor_FC"},
      { "layerType":"SoftMax",
        "layerName":"Logits",
        "layerInput":"StopNan"},
      { "layerType":"LogSoftMax",
        "layerName":"LogLogits",
        "layerInput":"Actor_FC"}
    ],

    "Critic":[
      { "layerType":"Dense",
        "layerName":"Critic_FC1",
        "layerInput":"Dense2",
        "Parameters":{"units":512,
                      "activation":"relu"}},
      { "layerType":"Dense",
        "layerName":"Critic_FC2",
        "layerInput":"Critic_FC1",
        "Parameters":{"units":512,
                      "activation":"relu"}},
      { "layerType":"Dense",
        "layerName":"Critic_FC3",
        "layerInput":"Critic_FC2",
        "Parameters":{"units":1,
                      "activation":null}}
    ]
  },
  "NetworkOutputs":{
    "actor":"Logits",
    "critic":"Critic_FC3",
    "log_logits":"LogLogits"
  },
  "NetworkVariableGroups":{
    "Actor":["Actor","Shared"],
    "Critic":["Critic","Shared"]
  },
  "TestInput":{"state":[null,7,7,4]}

  }
