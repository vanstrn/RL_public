{ "NetworkName" : "MG4R_AC_v3",
  "NetworkBuilder":"network",
  "DefaultParams":{ "SFSize":256,
                    "Filters":128,
                    "KernalSize":3,
                    "FCUnits":256},
  "NetworkStructure":{
    "Shared":[
      { "layerType":"BasicCNNSplit",
        "layerName":"InputSplit",
        "layerInput":"input.state",
        "multiOutput":["in_background","in_agent"],
        "Parameters":{"division":1}},
      { "layerType":"Conv2D",
        "layerName":"Conv2D_1",
        "layerInput":"in_background",
        "Parameters":{"filters":"Filters//4",
                      "kernel_size":"KernalSize*2",
                      "strides":2,
                      "padding":"same",
                      "activation":"relu"}},
      { "layerType":"Conv2D",
        "layerName":"Conv2D_2",
        "layerInput":"Conv2D_1",
        "Parameters":{"filters":"Filters//2",
                      "kernel_size":"KernalSize",
                      "strides":1,
                      "activation":"relu"}},
      { "layerType":"Conv2D",
        "layerName":"Conv2D_3",
        "layerInput":"Conv2D_2",
        "Parameters":{"filters":"Filters",
                      "kernel_size":"KernalSize",
                      "strides":1,
                      "activation":"relu"}},
      { "layerType":"Flatten",
        "layerName":"Flatten",
        "layerInput":"Conv2D_3"},

      { "layerType":"Conv2D",
        "layerName":"Conv2D_1_agent",
        "layerInput":"in_agent",
        "Parameters":{"filters":"Filters//4",
                      "kernel_size":"KernalSize*2",
                      "strides":2,
                      "padding":"same",
                      "activation":"relu"}},
      { "layerType":"Conv2D",
        "layerName":"Conv2D_2_agent",
        "layerInput":"Conv2D_1_agent",
        "Parameters":{"filters":"Filters//2",
                      "kernel_size":"KernalSize",
                      "strides":1,
                      "activation":"relu"}},
      { "layerType":"Conv2D",
        "layerName":"Conv2D_3_agent",
        "layerInput":"Conv2D_2_agent",
        "Parameters":{"filters":"Filters",
                      "kernel_size":"KernalSize",
                      "strides":1,
                      "activation":"relu"}},
      { "layerType":"Flatten",
        "layerName":"Flatten_agent",
        "layerInput":"Conv2D_3_agent"},

      { "layerType":"Concatenate",
        "layerName":"Concatenate",
        "layerInput":["Flatten","Flatten_agent"],
        "Parameters":{}},

      { "layerType":"Dense",
        "layerName":"Dense1",
        "layerInput":"Concatenate",
        "Parameters":{"units":"FCUnits",
                      "activation":"relu"}},
      { "layerType":"Dense",
        "layerName":"Dense2",
        "layerInput":"Dense1",
        "Parameters":{"units":"SFSize",
                      "activation":null}}
      ],


    "Critic":[
      { "layerType":"Dense",
        "layerName":"Critic_FC1",
        "layerInput":"Dense2",
        "Parameters":{"units":"FCUnits",
                      "activation":"relu"}},
      { "layerType":"Dense",
        "layerName":"Critic_FC2",
        "layerInput":"Critic_FC1",
        "Parameters":{"units":1,
                      "activation":null}}
    ]
  },
  "NetworkOutputs":{
    "Q":"Critic_FC2"
  },
  "NetworkVariableGroups":{
    "Critic":["Critic","Shared"]
  },
  "TestInput":{"state":[null,7,7,4]}

  }
