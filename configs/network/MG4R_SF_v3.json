{ "NetworkName" : "MG_SF",
  "NetworkBuilder":"network",
  "DefaultParams":{"SFSize":64},
  "NetworkStructure":{
    "Shared":[
      { "layerType":"Conv2D",
        "layerName":"Conv2D_1",
        "layerInput":"input.state",
        "Parameters":{"filters":16,
                      "kernel_size":2,
                      "strides":1,
                      "activation":"relu"}},
      { "layerType":"MaxPooling2D",
        "layerName":"MaxPooling",
        "layerInput":"Conv2D_1",
        "Parameters":{"pool_size":2,
                      "strides":1}},
      { "layerType":"Conv2D",
        "layerName":"Conv2D_2",
        "layerInput":"MaxPooling",
        "Parameters":{"filters":32,
                      "kernel_size":2,
                      "strides":1,
                      "activation":"relu"}},
      { "layerType":"Conv2D",
        "layerName":"Conv2D_3",
        "layerInput":"Conv2D_2",
        "Parameters":{"filters":64,
                      "kernel_size":2,
                      "strides":1,
                      "activation":"relu"}},
      { "layerType":"Flatten",
        "layerName":"Flatten",
        "layerInput":"Conv2D_3"},
      { "layerType":"Dense",
        "layerName":"Dense1",
        "layerInput":"Flatten",
        "Parameters":{"units":"SFSize",
                      "activation":"relu"}}
      ],

    "SucessorRepresentation":[
      { "layerType":"Dense",
        "layerName":"SuccessorWeights",
        "layerInput":"Dense1",
        "Parameters":{"units":1,
                      "activation":null}}
      ],

    "Actor":[
      { "layerType":"Dense",
        "layerName":"Actor1",
        "layerInput":"Dense1",
        "Parameters":{"units":64,
                      "activation":"relu"}},
      { "layerType":"Dense",
        "layerName":"Actor2",
        "layerInput":"Actor1",
        "Parameters":{"units":"actionSize",
                      "activation":"relu"}},
      { "layerType":"SoftMax",
        "layerName":"Logits",
        "layerInput":"Actor2"},
      { "layerType":"LogSoftMax",
        "layerName":"LogLogits",
        "layerInput":"Actor2"}
    ],

    "Critic":[
      { "layerType":"Dense",
        "layerName":"Actor1",
        "layerInput":"Dense1",
        "Parameters":{"units":"SFSize",
                      "activation":"relu"}},
      { "layerType":"Dense",
        "layerName":"Critic_FC1",
        "layerInput":"Dense1",
        "Parameters":{"units":"SFSize",
                      "activation":"relu"}}
    ],

    "Critic_SF":[
      { "layerType":"Dense",
        "layerName":"Critic_FC2",
        "layerInput":"Critic_FC1",
        "ReuseLayer":"SuccessorWeights"}
    ],

    "Reconstruction":[
      { "layerType":"Concatenate",
        "layerName":"Reconstruction_Concatenate",
        "layerInput":["Dense1","Logits"],
        "Parameters":{"axis":1}},
      { "layerType":"Dense",
        "layerName":"Reconstruction_Dense",
        "layerInput":"Reconstruction_Concatenate",
        "Parameters":{"units":144,
                      "activation":"relu"}},
      { "layerType":"Reshape",
        "layerName":"Reconstruction_Reshape",
        "Parameters":{"target_shape":[3,3,16]},
        "layerInput":"Reconstruction_Dense"},
      { "layerType":"Conv2DTranspose",
        "layerName":"Reconstruction_Conv1",
        "layerInput":"Reconstruction_Reshape",
        "Parameters":{"filters":32,
                      "kernel_size":3,
                      "strides":3,
                      "activation":"relu"}},
      { "layerType":"Conv2DTranspose",
        "layerName":"Reconstruction_Conv2",
        "layerInput":"Reconstruction_Conv1",
        "Parameters":{"filters":16,
                      "kernel_size":2,
                      "strides":2,
                      "activation":"relu"}},
      { "layerType":"Conv2DTranspose",
        "layerName":"Reconstruction_Conv3",
        "layerInput":"Reconstruction_Conv2",
        "Parameters":{"filters":2,
                      "kernel_size":2,
                      "strides":1,
                      "activation":"relu"}},
      { "layerType":"Round",
        "layerName":"Reconstruction_Round",
        "layerInput":"Reconstruction_Conv3"}
    ]

  },
  "NetworkOutputs":{
    "actor":"Logits",
    "critic":"Critic_FC2",
    "log_logits":"LogLogits",
    "prediction":"Reconstruction_Round",
    "phi":"Dense1",
    "psi":"Critic_FC1",
    "reward_pred":"SuccessorWeights"
  },

  "NetworkVariableGroups":{
    "Actor":["Actor"],
    "Critic":["Critic"],
    "Reward":["SucessorRepresentation","Shared"],
    "Reconstruction":["Reconstruction","Shared"]
  },
  "TestInput":{"state":[null,7,7,4]}

  }
