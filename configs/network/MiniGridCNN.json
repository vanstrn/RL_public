{ "NetworkName" : "DefaultCtF",
  "NetworkStructure":{
    "Shared":[
      { "layerType":"Conv2D",
        "layerName":"Conv2D_1",
        "layerInput":"input.state",
        "filters":32,
        "kernel_size":2,
        "strides":1,
        "activation":"relu"},
      { "layerType":"Conv2D",
        "layerName":"Conv2D_2",
        "layerInput":"Conv2D_1",
        "filters":64,
        "kernel_size":2,
        "strides":1,
        "activation":"relu"},
      { "layerType":"Flatten",
        "layerName":"Flatten",
        "layerInput":"Conv2D_2"},
      { "layerType":"Dense",
        "layerName":"Dense1",
        "layerInput":"Flatten",
        "outputSize":256,
        "activation":null}
      ],

    "Actor":[
      { "layerType":"Dense",
        "layerName":"Actor_FC1",
        "layerInput":"Dense1",
        "outputSize":"64",
        "activation":null},
      { "layerType":"Dense",
        "layerName":"Actor_FC2",
        "layerInput":"Actor_FC1",
        "outputSize":"actionSize",
        "activation":null},
      { "layerType":"SoftMax",
        "layerName":"Logits",
        "layerInput":"Actor_FC2"},
      { "layerType":"LogSoftMax",
        "layerName":"LogLogits",
        "layerInput":"Actor_FC2"}
    ],

    "Critic":[
      { "layerType":"Dense",
        "layerName":"Critic_FC1",
        "layerInput":"Dense1",
        "outputSize":1,
        "activation":null}
    ]
  },
  "NetworkOutputs":{
    "actor":"Logits",
    "critic":"Critic_FC1",
    "log_logits":"LogLogits"
  },
  "NetworkVariableGroups":{
    "Actor":["Actor","Shared"],
    "Critic":["Critic","Shared"]
  }

  }
