{ "NetworkName" : "DefaultCtF",
  "NetworkStructure":{
    "Shared":[
      { "layerType":"Conv2D",
        "layerName":"Conv2D_1",
        "layerInput":"input.state",
        "filters":16,
        "kernel_size":3,
        "strides":2,
        "activation":"relu"},
      { "layerType":"Conv2D",
        "layerName":"Conv2D_2",
        "layerInput":"Conv2D_1",
        "filters":32,
        "kernel_size":3,
        "strides":2,
        "activation":"relu"},
      { "layerType":"Flatten",
        "layerName":"Flatten",
        "layerInput":"Conv2D_2"},
      { "layerType":"Reshape",
        "layerName":"Reshape",
        "layerInput":"Flatten",
        "target_shape":[1,32]},
      { "layerType":"LSTM",
        "layerName":"LSTM1",
        "layerInput":["Reshape","input.hiddenState"],
        "multiOutput":["layer","hidden_s","hidden_c"],
        "Parameters":{"units":64,
                      "activation":"relu",
                      "recurrent_activation":"sigmoid"}},
      { "layerType":"Dense",
        "layerName":"Dense1",
        "layerInput":"LSTM",
        "outputSize":128,
        "activation":null}
      ],

    "Actor":[
      { "layerType":"Dense",
        "layerName":"Actor_FC",
        "layerInput":"Dense1",
        "outputSize":"actionSize",
        "activation":null},
      { "layerType":"SoftMax",
        "layerName":"Logits",
        "layerInput":"Actor_FC"},
      { "layerType":"LogSoftMax",
        "layerName":"LogLogits",
        "layerInput":"Actor_FC"}
    ],

    "Critic":[
      { "layerType":"Dense",
        "layerName":"Critic_FC1",
        "layerInput":"Dense1",
        "outputSize":1,
        "activation":null}
    ]
  },
  "NetworkOutputs":{
    "actor":"Logits",
    "critic":"Critic_FC1",
    "log_logits":"LogLogits"
  },
  "NetworkVariableGroups":{
    "Actor":["Actor","Shared"],
    "Critic":["Critic","Shared"]
  }

  }
