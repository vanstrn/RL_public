{ "NetworkName" : "DefaultCtF",
  "Description":"Hierarchical Network where the feature encoder is shared by all sub-networks. Uses shared network structure to ",
  "NumOptions":3,
  "NetworkStructure":{
    "SharedNetworkStructure":{
      "FeatureEncoder":[
      { "layerType":"SeparableConv",
        "layerName":"SeparableConv",
        "layerInput":"input.state",
        "filters":32,
        "kernel_size":4,
        "strides":2,
        "padding":"valid",
        "depth_multiplier":4,
        "activation":"relu"},
      { "layerType":"Conv2D",
        "layerName":"Conv2D_1",
        "layerInput":"SeparableConv",
        "filters":64,
        "kernel_size":3,
        "strides":2,
        "activation":"relu"},
      { "layerType":"Conv2D",
        "layerName":"Conv2D_2",
        "layerInput":"Conv2D_1",
        "filters":64,
        "kernel_size":3,
        "strides":2,
        "activation":"relu"},
      { "layerType":"Flatten",
        "layerName":"Flatten",
        "layerInput":"Conv2D_2"},
      { "layerType":"Dense",
        "layerName":"Dense1",
        "layerInput":"Flatten",
        "outputSize":128,
        "activation":null}
      ]
    },

  "MetaNetworkStructure":{
    "Actor":[
      { "layerType":"Dense",
        "layerName":"Meta_Actor_FC1",
        "layerInput":"Dense1",
        "outputSize":"actionSize",
        "activation":null},
      { "layerType":"SoftMax",
        "layerName":"Meta_Actor_logits",
        "layerInput":"Meta_Actor_FC1"},
      { "layerType":"LogSoftMax",
        "layerName":"Meta_Actor_LogLogits",
        "layerInput":"Meta_Actor_logits"}
    ],
    "Critic":[
      { "layerType":"Dense",
        "layerName":"MetaCritic",
        "layerInput":"Dense1",
        "outputSize":1,
        "activation":null}
    ]
  },

  "SubNetworkStructure":{

    "Actor":[
      { "layerType":"Dense",
        "layerName":"Sub_Actor_FC1",
        "layerInput":"Dense1",
        "outputSize":"actionSize",
        "activation":null},
      { "layerType":"SoftMax",
        "layerName":"Sub_Actor_logits",
        "layerInput":"Sub_Actor_FC1"},
      { "layerType":"LogSoftMax",
        "layerName":"Sub_Actor_LogLogits",
        "layerInput":"Sub_Actor_logits"}
    ],

    "Critic":[
      { "layerType":"Dense",
        "layerName":"SubCritic",
        "layerInput":"Dense1",
        "outputSize":1,
        "activation":null}
    ]
  }
  },
  "NetworkOutputs":{
    "metaActor":"Meta_Actor_logits",
    "metaLogLogits":"Meta_Actor_LogLogits",
    "metaCritic":"MetaCritic",
    "subActor":"Sub_Actor_logits",
    "subLogLogits":"Sub_Actor_LogLogits",
    "subCritic":"SubCritic"
  },
  "NetworkVariableGroups":{
    "Actor":["Actor","Shared"],
    "Critic":["Critic","Shared"]
  }

  }
