{ "NetworkName" : "CTF_PPO",
  "NetworkBuilder":"network",
  "NetworkStructure":{
    "Shared":[
      { "layerType":"SeparableConv",
        "layerName":"SepConv",
        "layerInput":"input.state",
        "Parameters":{"filters":32,
                      "kernel_size":6,
                      "strides":2,
                      "depth_multiplier":4,
                      "activation":"relu"}},
      { "layerType":"NonLocalNN",
        "layerName":"NonLocal",
        "layerInput":"SepConv",
        "Parameters":{"filters":32}},
      { "layerType":"Conv2D",
        "layerName":"Conv2D_1",
        "layerInput":"NonLocal",
        "Parameters":{"filters":32,
                      "kernel_size":3,
                      "strides":1,
                      "activation":"relu"}},
      { "layerType":"Conv2D",
        "layerName":"Conv2D_2",
        "layerInput":"Conv2D_1",
        "Parameters":{"filters":64,
                      "kernel_size":3,
                      "strides":1,
                      "activation":"relu"}},
      { "layerType":"Conv2D",
        "layerName":"Conv2D_3",
        "layerInput":"Conv2D_2",
        "Parameters":{"filters":128,
                      "kernel_size":3,
                      "strides":1,
                      "activation":"relu"}},
      { "layerType":"Flatten",
        "layerName":"Flatten",
        "layerInput":"Conv2D_3"},
      { "layerType":"Dense",
        "layerName":"Dense1",
        "layerInput":"Flatten",
        "Parameters":{"units":512,
                      "activation":"relu"}}
      ],
    "Actor":[
      { "layerType":"Dense",
        "layerName":"Actor_FC",
        "layerInput":"Dense1",
        "Parameters":{"units":512,
                      "activation":"relu"}},
      { "layerType":"Dense",
        "layerName":"Actor_FC",
        "layerInput":"Dense1",
        "Parameters":{"units":"actionSize",
                      "activation":null}},
      { "layerType":"StopNan",
        "layerName":"StopNan",
        "layerInput":"Actor_FC"},
      { "layerType":"SoftMax",
        "layerName":"Logits",
        "layerInput":"StopNan"},
      { "layerType":"LogSoftMax",
        "layerName":"LogLogits",
        "layerInput":"Actor_FC"}
    ],

    "Critic":[
      { "layerType":"Dense",
        "layerName":"Critic_FC1",
        "layerInput":"Dense1",
        "Parameters":{"units":512,
                      "activation":"relu"}},
      { "layerType":"Dense",
        "layerName":"Critic_FC2",
        "layerInput":"Critic_FC1",
        "Parameters":{"units":1,
                      "activation":null}}
    ]
  },
  "NetworkOutputs":{
    "actor":"Logits",
    "critic":"Critic_FC2",
    "log_logits":"LogLogits"
  },
  "NetworkVariableGroups":{
    "Actor":["Actor","Shared"],
    "Critic":["Critic","Shared"]
  },
  "TestInput":{"state":[null,7,7,4]}

  }
