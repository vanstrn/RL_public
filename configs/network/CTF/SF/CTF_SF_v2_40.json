{ "NetworkName" : "MG_SF",
  "NetworkBuilder":"network",
  "DefaultParams":{ "SFSize":1024,
                    "Trainable":true,
                    "InFilter":64,
                    "OutFilter":64,
                    "FCUnits":1024},
  "Inputs":{"state":[40,240,6]},
  "NetworkStructure":{
    "Shared":[
      { "layerType":"Conv2D",
        "layerName":"Conv1",
        "layerInput":"input.state",
        "Parameters":{"filters":32,
                      "kernel_size":6,
                      "strides":2,
                      "activation":"relu"}},
      { "layerType":"Conv2D",
        "layerName":"Conv2",
        "layerInput":"Conv1",
        "Parameters":{"filters":64,
                      "kernel_size":6,
                      "strides":2,
                      "activation":"relu"}},
      { "layerType":"NonLocalNN",
        "layerName":"NonLocal",
        "layerInput":"Conv2",
        "Parameters":{"filters":64}},
      { "layerType":"Conv2D",
        "layerName":"Conv2D_1",
        "layerInput":"NonLocal",
        "Parameters":{"filters":128,
                      "kernel_size":3,
                      "strides":1,
                      "activation":"relu"}},
      { "layerType":"Conv2D",
        "layerName":"Conv2D_2",
        "layerInput":"Conv2D_1",
        "Parameters":{"filters":256,
                      "kernel_size":3,
                      "strides":1,
                      "activation":"relu"}},
      { "layerType":"Conv2D",
        "layerName":"Conv2D_3",
        "layerInput":"Conv2D_2",
        "Parameters":{"filters":512,
                      "kernel_size":3,
                      "strides":1,
                      "activation":"relu"}},
      { "layerType":"Flatten",
        "layerName":"Flatten",
        "layerInput":"Conv2D_3"},
      { "layerType":"Dense",
        "layerName":"Phi",
        "layerInput":"Dense1",
        "Parameters":{"units":"SFSize",
                      "activation":"relu",
                      "trainable":"Trainable"}}
      ],

    "SucessorRepresentation":[
      { "layerType":"Dense",
        "layerName":"SuccessorWeights",
        "layerInput":"Phi",
        "Parameters":{"units":1,
                      "activation":null,
                      "trainable":"Trainable"}}
      ],


    "Critic":[
      { "layerType":"Dense",
        "layerName":"Critic_FC1",
        "layerInput":"Phi",
        "Parameters":{"units":"FCUnits",
                      "activation":"relu"}},
      { "layerType":"Dense",
        "layerName":"Critic_FC2",
        "layerInput":"Critic_FC1",
        "Parameters":{"units":"FCUnits",
                      "activation":"relu"}},
      { "layerType":"Dense",
        "layerName":"Psi",
        "layerInput":"Critic_FC2",
        "Parameters":{"units":"SFSize",
                      "activation":null}}
    ],

    "Critic_SF":[
      { "layerType":"Dense",
        "layerName":"Critic",
        "layerInput":"Psi",
        "ReuseLayer":"SuccessorWeights"}
    ],

    "Reconstruction":[
      { "layerType":"Dense",
        "layerName":"Reconstruction_Dense",
        "layerInput":"Phi",
        "Parameters":{"units":1296,
                      "activation":"relu"}},
      { "layerType":"Reshape",
        "layerName":"Reconstruction_Reshape",
        "Parameters":{"target_shape":[9,9,16]},
        "layerInput":"Reconstruction_Dense"},
      { "layerType":"Conv2DTranspose",
        "layerName":"Reconstruction_Conv1",
        "layerInput":"Reconstruction_Reshape",
        "Parameters":{"filters":128,
                      "kernel_size":6,
                      "strides":2,
                      "activation":"relu"}},
      { "layerType":"Conv2DTranspose",
        "layerName":"Reconstruction_Conv2",
        "layerInput":"Reconstruction_Conv1",
        "Parameters":{"filters":128,
                      "kernel_size":3,
                      "strides":1,
                      "activation":"relu"}},
      { "layerType":"Conv2DTranspose",
        "layerName":"Reconstruction_Conv3",
        "layerInput":"Reconstruction_Conv2",
        "Parameters":{"filters":64,
                      "kernel_size":3,
                      "strides":1,
                      "activation":"relu"}},
      { "layerType":"Conv2DTranspose",
        "layerName":"StatePrediction",
        "layerInput":"Reconstruction_Conv3",
        "Parameters":{"filters":6,
                      "kernel_size":3,
                      "strides":1,
                      "padding":"valid",
                      "activation":null}}
    ]

  },
  "NetworkOutputs":[
    {
      "prediction":"StatePrediction",
      "reward_pred":"SuccessorWeights"
    },
    {
      "psi":"Psi"
    },
    {
      "phi":"Phi"
    },
    {
      "critic":"Critic"
    },
    {
      "phi":"Phi",
      "prediction":"StatePrediction",
      "reward_pred":"SuccessorWeights",
      "psi":"Psi",
      "critic":"Critic"
    }
  ],

  "NetworkVariableGroups":{
    "Actor":["Actor"],
    "Critic":["Critic"],
    "Reward":["SucessorRepresentation","Shared"],
    "Reconstruction":["Reconstruction","Shared"]
  },
  "TestInput":{"state":[null,7,7,4]}

  }
