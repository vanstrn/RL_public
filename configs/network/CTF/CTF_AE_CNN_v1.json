{ "NetworkName" : "MG_SF",
  "NetworkBuilder":"network",
  "DefaultParams":{"SFSize":400},
  "NetworkStructure":{
    "Shared":[
      { "layerType":"Conv2D",
        "layerName":"Conv2D_1",
        "layerInput":"input.state",
        "Parameters":{"filters":64,
                      "kernel_size":6,
                      "strides":2,
                      "activation":"elu"}},
      { "layerType":"Conv2D",
        "layerName":"Conv2D_2",
        "layerInput":"Conv2D_1",
        "Parameters":{"filters":64,
                      "kernel_size":4,
                      "strides":2,
                      "activation":"elu"}},
      { "layerType":"Conv2D",
        "layerName":"Conv2D_3",
        "layerInput":"Conv2D_2",
        "Parameters":{"filters":128,
                      "kernel_size":3,
                      "strides":1,
                      "activation":"elu"}},
      { "layerType":"Conv2D",
        "layerName":"Conv2D_4",
        "layerInput":"Conv2D_3",
        "Parameters":{"filters":128,
                      "kernel_size":3,
                      "strides":1,
                      "activation":"elu"}},
      { "layerType":"Conv2D",
        "layerName":"Conv2D_5",
        "layerInput":"Conv2D_4",
        "Parameters":{"filters":128,
                      "kernel_size":3,
                      "strides":1,
                      "activation":"elu"}},
      { "layerType":"Flatten",
        "layerName":"Flatten",
        "layerInput":"Conv2D_5",
        "Parameters":{}},
      { "layerType":"Dense",
        "layerName":"Dense1",
        "layerInput":"Flatten",
        "Parameters":{"units":"SFSize",
                      "activation":"elu"}}
      ],

    "Reconstruction":[
      { "layerType":"Dense",
        "layerName":"Reconstruction_Dense",
        "layerInput":"Dense1",
        "Parameters":{"units":968,
                      "activation":"elu"}},
      { "layerType":"Reshape",
        "layerName":"Reconstruction_Reshape",
        "Parameters":{"target_shape":[11,11,8]},
        "layerInput":"Reconstruction_Dense"},
      { "layerType":"Conv2DTranspose",
        "layerName":"Reconstruction_Conv1",
        "layerInput":"Reconstruction_Reshape",
        "Parameters":{"filters":32,
                      "kernel_size":6,
                      "strides":2,
                      "activation":"elu"}},
      { "layerType":"Conv2DTranspose",
        "layerName":"Reconstruction_Conv2",
        "layerInput":"Reconstruction_Conv1",
        "Parameters":{"filters":64,
                      "kernel_size":6,
                      "strides":1,
                      "activation":"elu"}},
      { "layerType":"Conv2DTranspose",
        "layerName":"Reconstruction_Conv3",
        "layerInput":"Reconstruction_Conv2",
        "Parameters":{"filters":64,
                      "kernel_size":3,
                      "strides":1,
                      "activation":"elu"}},
      { "layerType":"Conv2DTranspose",
        "layerName":"Reconstruction_Conv4",
        "layerInput":"Reconstruction_Conv3",
        "Parameters":{"filters":64,
                      "kernel_size":3,
                      "strides":1,
                      "activation":"elu"}},
      { "layerType":"Conv2DTranspose",
        "layerName":"Reconstruction_Conv5",
        "layerInput":"Reconstruction_Conv4",
        "Parameters":{"filters":64,
                      "kernel_size":3,
                      "strides":1,
                      "activation":"elu"}},
      { "layerType":"Conv2DTranspose",
        "layerName":"Reconstruction_Conv6",
        "layerInput":"Reconstruction_Conv5",
        "Parameters":{"filters":7,
                      "kernel_size":3,
                      "strides":1,
                      "activation":"elu"}}
    ]

  },
  "NetworkOutputs":{
    "prediction":"Reconstruction_Conv6",
    "phi":"Dense1"
  },

  "NetworkVariableGroups":{
    "Reconstruction":["Reconstruction","Shared"]
  },
  "TestInput":{"state":[null,7,7,4]}

  }
