{ "NetworkName" : "CTF_PPO",
  "NetworkBuilder":"network",
  "NetworkStructure":{
    "Shared":[
      { "layerType":"Conv2D",
        "layerName":"Conv2D_1",
        "layerInput":"input.state",
        "Parameters":{"filters":32,
                      "kernel_size":6,
                      "strides":2,
                      "padding":"same",
                      "activation":"relu"}},
      { "layerType":"Inception",
        "layerName":"Inception1",
        "layerInput":"Conv2D_1",
        "Parameters":{"filters":[32,32,16,16]}},
      { "layerType":"Inception",
        "layerName":"Inception2",
        "layerInput":"Inception1",
        "Parameters":{"filters":[64,64,32,32]}},
      { "layerType":"Inception",
        "layerName":"Inception3",
        "layerInput":"Inception2",
        "Parameters":{"filters":[128,128,64,64]}},
      { "layerType":"Inception",
        "layerName":"Inception4",
        "layerInput":"Inception3",
        "Parameters":{"filters":[256,256,128,128]}},
      { "layerType":"GlobalAveragePooling2D",
        "layerName":"AveragePool",
        "layerInput":"Inception4",
        "Parameters":{"filters":[5,5]}},
      { "layerType":"Flatten",
        "layerName":"Flatten",
        "layerInput":"AveragePool"},
      { "layerType":"Dense",
        "layerName":"Dense1",
        "layerInput":"Flatten",
        "Parameters":{"units":512,
                      "activation":"relu"}}
      ],
    "Actor":[
      { "layerType":"Dense",
        "layerName":"Actor_FC1",
        "layerInput":"Dense1",
        "Parameters":{"units":512,
                      "activation":"relu"}},
      { "layerType":"Dense",
        "layerName":"Actor_FC2",
        "layerInput":"Actor_FC1",
        "Parameters":{"units":"actionSize",
                      "activation":null}},
      { "layerType":"StopNan",
        "layerName":"StopNan",
        "layerInput":"Actor_FC2"},
      { "layerType":"SoftMax",
        "layerName":"Logits",
        "layerInput":"StopNan"},
      { "layerType":"LogSoftMax",
        "layerName":"LogLogits",
        "layerInput":"Actor_FC2"}
    ],

    "Critic":[
      { "layerType":"Dense",
        "layerName":"Critic_FC1",
        "layerInput":"Dense1",
        "Parameters":{"units":512,
                      "activation":"relu"}},
      { "layerType":"Dense",
        "layerName":"Critic_FC2",
        "layerInput":"Critic_FC1",
        "Parameters":{"units":1,
                      "activation":null}}
    ]
  },
  "NetworkOutputs":{
    "actor":"Logits",
    "critic":"Critic_FC2",
    "log_logits":"LogLogits"
  },
  "NetworkVariableGroups":{
    "Actor":["Actor","Shared"],
    "Critic":["Critic","Shared"]
  },
  "TestInput":{"state":[null,7,7,4]}

  }
