{ "NetworkName" : "MG_SFOP",
  "NetworkBuilder":"network",
  "DefaultParams":{"SFSize":36},
  "NetworkStructure":{
    "Shared":[
      { "layerType":"Conv2D",
        "layerName":"Conv2D_1",
        "layerInput":"input.state",
        "Parameters":{"filters":16,
                      "kernel_size":2,
                      "strides":1,
                      "activation":"relu"}},
      { "layerType":"Conv2D",
        "layerName":"Conv2D_2",
        "layerInput":"Conv2D_1",
        "Parameters":{"filters":32,
                      "kernel_size":2,
                      "strides":1,
                      "activation":"relu"}},
      { "layerType":"Flatten",
        "layerName":"Flatten",
        "layerInput":"Conv2D_2"},
      { "layerType":"Dense",
        "layerName":"Dense1",
        "layerInput":"Flatten",
        "Parameters":{"units":"SFSize",
                      "activation":"relu"}}
      ],

    "SucessorRepresentation":[
      { "layerType":"Dense",
        "layerName":"SuccessorWeights",
        "layerInput":"Dense1",
        "Parameters":{"units":1,
                      "activation":null}}
      ],


    "Critic":[
      { "layerType":"Dense",
        "layerName":"Critic_FC1",
        "layerInput":"Dense1",
        "Parameters":{"units":"SFSize",
                      "activation":"relu"}}
    ],

    "Critic_SF":[
      { "layerType":"Dense",
        "layerName":"Critic_FC2",
        "layerInput":"Critic_FC1",
        "ReuseLayer":"SuccessorWeights"}
    ],

    "Reconstruction":[
      { "layerType":"Dense",
        "layerName":"Reconstruction_Dense",
        "layerInput":"Dense1",
        "Parameters":{"units":256,
                      "activation":"relu"}},
      { "layerType":"Reshape",
        "layerName":"Reconstruction_Reshape",
        "Parameters":{"target_shape":[4,4,16]},
        "layerInput":"Reconstruction_Dense"},
      { "layerType":"Conv2DTranspose",
        "layerName":"Reconstruction_Conv1",
        "layerInput":"Reconstruction_Reshape",
        "Parameters":{"filters":16,
                      "kernel_size":3,
                      "strides":1,
                      "activation":"relu"}},
      { "layerType":"Conv2DTranspose",
        "layerName":"Reconstruction_Conv2",
        "layerInput":"Reconstruction_Conv1",
        "Parameters":{"filters":8,
                      "kernel_size":2,
                      "strides":1,
                      "activation":"relu"}},
      { "layerType":"Conv2DTranspose",
        "layerName":"Reconstruction_Conv3",
        "layerInput":"Reconstruction_Conv2",
        "Parameters":{"filters":2,
                      "kernel_size":2,
                      "strides":1,
                      "activation":"relu"}},
      { "layerType":"Round",
        "layerName":"Reconstruction_Round",
        "layerInput":"Reconstruction_Conv3"}
    ]

  },
  "NetworkOutputs":{
    "critic":"Critic_FC2",
    "log_logits":"LogLogits",
    "prediction":"Reconstruction_Round",
    "phi":"Dense1",
    "psi":"Critic_FC1",
    "reward_pred":"SuccessorWeights"
  },

  "NetworkVariableGroups":{
    "Critic":["Critic"],
    "Reward":["Shared"],
    "Reconstruction":["Reconstruction","Shared"]
  },
  "TestInput":{"state":[null,7,7,4]}

  }
